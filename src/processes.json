{
    "apps": [
        {
            "name": "web",
            "cwd": "/home/node/src",
            "script": "/home/node/src/app.js",
            "log_date_format": "YYYY-MM-DD HH:mm",
            "error_file": "../logs/stderr.log",
            "out_file": "../logs/stdout.log",
            "pid_file": "/var/run/node_web.pid",
            "instances": 1,
            "min_uptime": "60s",
            "cron_restart": "*/2 * * * *",
            "merge_logs": true,
            "exec_interpreter": "node",
            "exec_mode": "cluster",
            "autorestart": true,
            "vizion": false
        }
    ]
}